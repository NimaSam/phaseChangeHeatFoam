FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    upDown
    {
	type	empty;	
    }
    right
    {
	type            inletOutlet;
	inletValue	uniform (0 0 0);
        value           uniform (0 0 0);  
    }	
    left
    {
	type            fixedValue;
        value           uniform (0 0 0);    
    }
    frontAndBack 
    {
	type	empty; 
    }

}

